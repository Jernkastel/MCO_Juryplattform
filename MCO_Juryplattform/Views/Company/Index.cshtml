@model IEnumerable<MCO_Juryplattform.Company>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
		<th>
			@Html.DisplayNameFor(model => model.Href)
		</th>
        
    </tr>

@foreach (var item in Model) {
    <tr>
		<td>
			@Html.ActionLink(item.Name, "Details", new { id = item.Id })
			</td>
		<td>
			@Html.ActionLink(item.Href, "Link", new { href = item.Href })
		</td>

    </tr>
}

</table>
